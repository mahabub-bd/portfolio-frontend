{
  "appName": "portfolio",
  " projectName": "mahabub-portfolio",
  "buildType": "dockerfile",
  "dockerfilePath": "./Dockerfile",
  "dockerContext": ".",
  "buildStage": "runner",
  "ports": [
    {
      "containerPort": 3000,
      "hostPort": 5000,
      "protocol": "http"
    }
  ],
  "environmentVariables": [
    {
      "key": "NODE_ENV",
      "value": "production"
    },
    {
      "key": "NEXT_TELEMETRY_DISABLED",
      "value": "1"
    }
  ],
  "domains": [
    {
      "host": "mahabub.me",
      "port": 5000,
      "https": true
    }
  ],
  "healthCheck": {
    "enabled": true,
    "path": "/",
    "interval": 30000,
    "timeout": 10000,
    "retries": 3
  }
}
